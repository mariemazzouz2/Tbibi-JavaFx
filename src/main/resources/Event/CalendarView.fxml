<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Evenement.CalendarViewController"
      spacing="10" styleClass="window" stylesheets="@style.css">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <WebView fx:id="webView" prefHeight="400.0" prefWidth="600.0" styleClass="calendar-webview"/>
    <Button text="Retour" onAction="#goBack" styleClass="action-button"/>
</VBox>